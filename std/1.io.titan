data IO a

val putChar : Char -> IO {}
val putStr : List Char -> IO {}
val putStrLn : List Char -> IO {}
val print : a -> IO {} where Show a

val getChar : IO Char
val getLine : IO (List Char)
val getContents : IO (List Char)

val readFile : List Char -> IO (List Char)
val writeFile : List Char -> List Char -> IO {}
val appendFile : List Char -> List Char -> IO {}
